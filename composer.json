{
    "name": "takeru-yamamoto/laravel-file",
    "description": "Easy to operate files for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "LaravelFile\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFile\\ServiceProvider"
            ],
            "aliases": {
                "File": "LaravelFile\\Facade\\File"
            }
        }
    },
    "authors": [
        {
            "name": "Takeru Yamamoto",
            "email": "yamamo9150@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "^11.0",
        "intervention/image": "^3.0",
        "pbmedia/laravel-ffmpeg": "^8.2",
        "phpoffice/phpspreadsheet": "^1.26"
    },
    "minimum-stability": "dev"
}